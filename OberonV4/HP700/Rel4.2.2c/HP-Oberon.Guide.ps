    origin Unix eq {60 10 translate} if
    origin Pluto eq {152 18 translate} if
    origin V3 eq {0 0 translate} if
  } if
  orientation Landscape eq {
    2480.3 0 translate 90 rotate
    origin Unix eq {7 42 translate} if
    origin Pluto eq {12 106 translate} if
    origin V3 eq {0 0 translate} if
  } if
  (Syntax10.Scn.Fnt) f
  0 setgray
} bdef

